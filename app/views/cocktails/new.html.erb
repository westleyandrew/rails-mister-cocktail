
<div class="container">
<div class="row">
<div class="col-6" >
  <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <button type="button" class="btn btn-primary"> <%= f.button :submit %> </button>
  <% end %>
</div>
</div>
</div>


