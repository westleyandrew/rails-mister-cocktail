<div class="container center_div">
  <h3>Add your own cocktail</h3>
  <div class="row"></div>
    <div class="booking-form new-cocktail" >
      <%= simple_form_for @cocktail do |f| %>
        <%= f.input :name %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <button type="button" class="btn btn-add-cocktail"> <%= f.button :submit %> </button>
      <% end %>
    </div>
  </div>
</div>
